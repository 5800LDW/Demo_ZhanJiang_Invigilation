<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
    </style>

    <!--横线样式-->
    <style name="style_tab_indicator">
        <item name="android:layout_width">@dimen/dimen_20</item>
        <item name="android:layout_height">@dimen/dimen_3</item>
        <item name="android:background">@color/c_white</item>
    </style>

    <!--横线样式-->
    <style name="style_h_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_1</item>
        <item name="android:background">@color/c_f3f3f3</item>
    </style>

