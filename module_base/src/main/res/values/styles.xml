        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/c_f3f3f3</item>
    </style>

    <!-- 标题栏style -->
    <style name="style_title_bar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:id">@id/title_bar</item>
    </style>

    <!--标签样式-->
    <style name="style_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/c_1c1c1c</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <!--白色按钮样式-->
    <style name="style_white_btn" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_white_bg_selector2</item>
        <item name="android:textColor">@color/c_1c1c1c</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:padding">@dimen/dimen_10</item>
